{
  "manifest_version": 3,
  "name": "Tiny Data Sender",
  "version": "1.0",
  "description": "Sends test data to localhost:8000/collect.",
  "permissions": [],
  "host_permissions": [
    "http://localhost/*",
    "http://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Send Data"
  }
}